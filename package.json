    "regenerator-runtime": "^0.11.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "script-loader": "^0.7.2",
    "shelljs": "^0.7.8",
    "standard": "*",
    "standardjs": "^1.0.0",
    "stylelint": "^8.3.1",
    "stylelint-webpack-plugin": "^0.9.0",
    "supertest": "^3.0.0",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "url-loader": "^0.6.2",
