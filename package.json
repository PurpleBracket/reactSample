    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "flow-bin": "^0.58.0",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "jest": "^21.2.1",
    "jest-css-modules": "^1.1.0",
    "jest-junit": "^3.3.0",
    "lerna": "^2.5.0",
    "mocha": "^4.0.1",
    "mocha-junit-reporter": "^1.15.0",
    "nock": "^9.1.4",
    "node-sass": "^4.7.2",
    "npm-watch": "^0.3.0",
    "nyc": "^11.4.0",
