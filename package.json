{
  "name": "treasure-api",
  "version": "1.0.0",
  "description": "Treasure Backend",
  "main": "./dist/server.js",
  "private": true,
  "scripts": {
    "build-dev": "rimraf dist bundle && webpack --env.CONFIG_ENV=development",
    "build-dev-watch": "rimraf dist bundle && webpack --watch --env.CONFIG_ENV=development",
    "build-local": "rimraf dist bundle && webpack --env.CONFIG_ENV=local",
    "build-local-watch": "rimraf dist bundle && webpack --watch --env.CONFIG_ENV=local",
    "build-prod": "rimraf dist bundle && NODE_ENV=production BABEL_ENV=production webpack --optimize-minimize -p --env.CONFIG_ENV=production --env.NODE_ENV=production",
    "bundle": "rimraf bundle.zip && cd bundle && zip -r9q ../bundle.zip ./*",
    "clean": "rimraf node_modules dist bundle",
    "cover": "npm run lint && npm run jest-cover",
    "__debug": "node --inspect-brk ./dist/server.js",
    "debug-test": "NODE_ENV=test node --inspect-brk ./node_modules/.bin/jest treasure-api/src --config jest.config.json",
    "deploy-master": "eb deploy treasure-api-dev",
    "deploy-production": "eb deploy treasure-api-prod",
    "__dev-debug": "NODE_ENV=development npm start",
    "dev-debug": "NODE_ENV=development node --inspect-brk ./dist/server.js",
    "dev-watch": "NODE_ENV=development nodemon --config nodemon.json --watch src",
    "dev-watch-debug": "NODE_ENV=development nodemon --inspect-brk ./dist/server.js --config nodemon.json --watch src",
    "local-debug": "NODE_ENV=local node --inspect-brk ./dist/server.js",
    "local-watch": "NODE_ENV=local nodemon --config nodemon.json --watch src",
    "local-watch-debug": "NODE_ENV=local nodemon --inspect-brk ./dist/server.js --config nodemon.json --watch src",
    "jest-cover": "nyc jest --ci --testResultsProcessor='jest-junit' treasure-api/src --config jest.config.json",
    "lint": "standard --fix",
    "lint-watch": "nodemon -d 2 -e js -x 'npm run lint'",
    "push": "npm run lint && git add . && git commit -m 'auto checkin' && git push",
    "start": "node ./dist/server.js",
    "start-prod": "NODE_ENV=production npm start",
    "start-dev": "NODE_ENV=development npm start",
    "start-local": "NODE_ENV=local npm start",
    "start-staging": "NODE_ENV=staging npm start",
    "start-test": "NODE_ENV=test npm start",
    "__serve-dev": "NODE_ENV=development webpack-dev-server --inline --hot --open",
    "test": "NODE_ENV=test jest treasure-api/src/ --config jest.config.json",
    "test-debug": "NODE_ENV=test ./node_modules/.bin/jest -i treasure-api/src --config jest.config.json",
    "test-watch": "NODE_ENV=test jest treasure-api/src/ --config jest.config.json --watch --notify",
    "validate": "npm run clean && npm run install && npm run build && npm run test && npm run deploy"
  },
  "watch": {
    "lint": {
      "test": "src/*.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theoutlander/treasure-api.git"
  },
  "author": "Nick Karnik",
  "license": "",
  "bugs": {
    "url": "https://github.com/theoutlander/treasure-api/issues"
  },
  "homepage": "https://github.com/theoutlander/treasure-api#readme",
  "dependencies": {
    "babel-plugin-add-react-displayname": "0.0.4",
    "babel-runtime": "^6.26.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "bulma": "^0.6.1",
    "bulma-extensions": "^0.5.2",
    "chalk": "^2.3.0",
    "confit": "^2.2.1",
